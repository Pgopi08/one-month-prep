   public static int sockMerchant(int n, List<Integer> ar) {
    // Write your code here
    int count=0;
    Collections.sort(ar);
      for(int i=0;i<n-1;){
          if(ar.get(i)==ar.get(i+1)){
              count++;
              i+=2;
          }
          else
          i++;
        //  return count;
      }
      return count;
    }

}
